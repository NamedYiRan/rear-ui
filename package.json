{
  "name": "rear-ui-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter @rear-ui/playground dev",
    "story": "pnpm --filter @rear-ui/playground storybook",
    "docs:dev": "pnpm --filter @rear-ui/docs dev",
    "docs:build": "cross-env NODE_ENV=production pnpm build && pnpm --filter @rear-ui/docs build",
    "test": "cross-env NODE_ENV=test pnpm --filter @rear-ui/components test",
    "build": "run-s build-hooks build-components",
    "build-components": "cross-env NODE_ENV=production pnpm --filter rear-ui build",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @rear-ui/hooks build",
    "build:dev": "run-s build-hooks:dev build-components:dev",
    "build-components:dev": "cross-env NODE_ENV=development pnpm --filter rear-ui build:watch",
    "build-hooks:dev": "cross-env NODE_ENV=development pnpm --filter @rear-ui/hooks build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@rear-ui/components": "workspace:*",
    "@rear-ui/constants": "workspace:*",
    "@rear-ui/hooks": "workspace:*",
    "@rear-ui/locale": "workspace:*",
    "@rear-ui/theme": "workspace:*",
    "@rear-ui/utils": "workspace:*",
    "@rear-ui/vite-plugins": "workspace:*",
    "@vitepress-preview/component": "workspace:*",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21",
    "rear-ui": "workspace:*",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.6",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "jsdom": "^24.1.3",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.2.0",
    "prettier": "^3.3.3",
    "rollup": "^4.27.3",
    "shelljs": "^0.8.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27",
    "vue3-i18n": "^1.1.5"
  }
}